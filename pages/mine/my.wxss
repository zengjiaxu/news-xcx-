/* pages/mine/my.wxss */
.userInfo{
  width: 750rpx;
  height:200rpx;
  padding: 15px;
  border: 1px solid #ccc;
  box-sizing: border-box
}
.img1{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 1px solid #ccc;
}
.userInfo .userFont{
  width: 400rpx;
  height: 200rpx;
  position: absolute;
  left: 160rpx;
  top: 20px;
  font-size: 13px
}
.userInfo .userFont .sex{
  color: #ccc;
  line-height: 60rpx
}
.shoucang{
  margin-top: 50rpx;
  margin-left: 20rpx;
  font-size: 30rpx;
  padding-left:10rpx;
  border-left: 4px solid red;
}
.listSC{
  width: 730rpx;
  background: #fff;
  border-bottom:1px solid #ccc;
  padding-bottom: 20rpx;
  margin: 10rpx;
  overflow: hidden;
  margin-top:50rpx
}
.listSC>.title{
  line-height: 50rpx;
}
.title{
  width: 440rpx;
  height:200rpx;
  /* padding: 10rpx;
  box-sizing: border-box; */
  float: left;
  position: relative
}
.title>.hours{
  color: #ccc;
  font-size: 30rpx;
  position: absolute;
  left:0;
  bottom: 0;
}
.imgBox{
  width: 290rpx;
  height: 200rpx;
  background: pink;
  float: left;
  /* padding: 10rpx; */
  /* box-sizing: border-box; */
}
.imgBox>image{
width: 100%;
height:100%;
}